# Sample Python Program

# Function to check even or odd
def check_even_odd(num):
    if num % 2 == 0:
        return "Even"
    else:
        return "Odd"

# Main program
def main():
    numbers = [10, 15, 22, 33, 40]

    print("Checking Even or Odd:\n")

    for n in numbers:
        result = check_even_odd(n)
        print(f"{n} is {result}")

if __name__ == "__main__":
    main()
